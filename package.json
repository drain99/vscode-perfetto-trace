{
  "name": "perfetto-trace",
  "displayName": "Perfetto Trace",
  "description": "Open perfetto-compatible trace files in perfetto ui within VS Code even for remote workspaces.",
  "version": "0.2.11",
  "license": "MIT",
  "homepage": "https://github.com/drain99/vscode-perfetto-trace",
  "repository": {
    "type": "git",
    "url": "https://github.com/drain99/vscode-perfetto-trace"
  },
  "publisher": "drain99",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "Perfetto Trace",
    "Chrome Trace"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "category": "Perfetto",
        "command": "perfetto-trace.open-trace-active-editor",
        "title": "Open Trace For Active Editor"
      },
      {
        "category": "Perfetto",
        "command": "perfetto-trace.open-trace-file",
        "title": "Open Trace For File"
      }
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run package",
    "clean": "rimraf out dist *.vsix"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.23.0",
    "glob": "^11.0.1",
    "mocha": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "vscode-uri": "^3.1.0"
  }
}
